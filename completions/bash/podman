    command_aliases=()

    commands=()
    commands+=("create")
    commands+=("exists")
    commands+=("inspect")
    commands+=("kill")
    commands+=("pause")
    commands+=("prune")
    commands+=("ps")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("list")
        aliashash["list"]="ps"
        command_aliases+=("ls")
        aliashash["ls"]="ps"
    fi
    commands+=("restart")
    commands+=("rm")
    commands+=("start")
    commands+=("stats")
    commands+=("stop")
    commands+=("top")
    commands+=("unpause")

